<section>
  <h1>Here's a home page.</h1>

  <h2>Articles</h2>
  <ul>
  {% for article in collections.writing %}
    <li>
      <a href="{{ article.url | url }}">{{ article.data.resource.title }}</a>
    </li>
  {% endfor %}
  </ul>

  <h2>Services</h2>
  <ul>
  {% for service in collections.services %}
    <li>
      <a href="{{ service.url | url }}">{{ service.data.resource.title }}</a>
    </li>
  {% endfor %}
  </ul>

  <h2>Case Studies</h2>
  <ul>
  {% for study in collections['case studies'] %}
    <li>
      <a href="{{ study.url | url }}">{{ study.data.resource.title }}</a>
    </li>
  {% endfor %}
  </ul>

</section>


