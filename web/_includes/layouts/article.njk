---
# Layout template for Article detail pages.
layout: layouts/base.njk
---
<article>
  <h1>{{ article.title }}</h1>
  <img src="{% articleHero article.heroImage %}" alt="{{ article.heroImage.altText }}">
  {{ article.body | markdownify | safe }}
  <p><a href="{{ '/' | url }}">‚Üê Home</a></p>
</article>